prime factor=[]
mrp=int(input('enter the amount:'))

for i in range (2,mrp+1):
  if mrp%i==0:
    a=0
    for j in range(2,i):
      if i%j==0:
        a=1
        break
    if a==0:
      prime_factor.append(i)  
print(prime_factor)
min=prime_factor[0]
alfipaid=min
roypaid=mrp-min
print(alfipaid)
print(roypaid)
            
