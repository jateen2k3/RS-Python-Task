def CountFrequency(my_list):
 
    # Creating an empty dictionary
    freq = {}
    for item in my_list:
        if (item in freq):
            freq[item] += 1
        else:
            freq[item] = 1
 
    for key, value in freq.items():
        print("% d : % d" % (key, value))
 
 
# Driver function
if name == "main":
    my_list = [1, 6, 8, 5, 5, 3, 1, 3, 2, 1, 0, 7, 7, 2, 2, 8,0]
 
    CountFrequency(my_list)
